{
        "name": "Preview on Wiki",
		"version": "1.0",
		"description": "Preview object on Wikipedia",
		"background": {
		  "scripts": ["background.js"],
		  "persistent": false
		},
		 "page_action": {
      "default_popup": "hello.html"
    },
        "permissions": [
          "contextMenus", "https://en.wikipedia.org/*", "activeTab", "tabs", "storage"
        ],
        "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
        },  
		"background": {
    "scripts": ["sample.js", "contentscript.js", "background.js"]
		},      
		 "manifest_version": 2
      }
      
	  